 def check\_tag\_names\_not\_empty(self, tag\_names):
 if not tag\_names:
 raise ValueError("tag\_names is empty")

 def check\_tags\_not\_empty(self, tags):
 if not tags:
 raise ValueError("tags is empty")

 def check\_tag\_in\_tag\_names(self, tags, tag\_names):
 for tag in tags:
 if tag not in tag\_names:
 return False
 return True

 def check\_performer\_tagged\_once(self, tags, tag\_names):
 # if len(set(tags).intersection(tag\_names)) > 1:
 # raise ValueError("Performer is tagged with more than one value in tag\_names- skipping")
 if not set(tags).intersection(tag\_names):
 return

 def upscale\_PerformerImage(self):
 global app\_keys
 # Create instance of TerminateProcess to terminate Gigapixel if already running
 tp = TerminateProcess(EXE\_PATH)
 # Start the thread and call terminate\_gigapixel
 tp.run\_terminate\_in\_thread()

 # Start Gigapixel instance through thread
 app = MainGigapixel(EXE\_PATH, OUTPUT\_SUFFIX)
 app.start\_gigapixel\_thread()

 # Continue
 self.check\_tag\_names\_not\_empty(tag\_names)
 for tag\_name in tag\_names:
 tag\_id = self.findTagIdWithName(tag\_name)
 if not tag\_id:
 self.debug("tag\_id: Is none, Removing tag: "+tag\_name)
 performers = self.findPerformersByTag(tag\_id)
 for performer in performers:
 self.process\_performer\_image(app, performer, tag\_names)

 def process\_performer\_image(self, app, performer, tag\_names):
 performer\_id = performer[0] # Initialize performer\_id here
 performer\_name = performer[1]
 tags = self.findPerformersTagsbyID(performer\_name)
 self.check\_performer\_tagged\_once(tags, tag\_names)
 self.check\_tags\_not\_empty(tags)
 for tag in tags:
 if tag != "Upscaled: Performer Image" and tag in tag\_names:
 self.error("if tag in tag\_names: "+tag)
